<!DOCTYPE html>
<html>

<head>
    <title>Neighborbrew Sign In</title>
    <!-- Font Awesome Icons -->
    <script defer src="https://use.fontawesome.com/releases/v5.0.9/js/all.js" integrity="sha384-8iPTk2s/jMVj81dnzb/iFR2sdA7u06vHJyyLlAd4snFpCl/SnyUjRrbdJsw1pGIl" crossorigin="anonymous"></script>

    <!-- UIkit CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-beta.41/css/uikit.min.css" />

    <!-- Custom CSS -->
    <link rel="stylesheet" type="text/css" href="assets/css/app.css">
</head>

<body>
  <header id="dashboard-header">
      <nav class="uk-navbar-container" uk-navbar>
        <div class="uk-navbar-right">
          <ul class="uk-navbar-nav">
            <li><a href="/logout">Go Back</a></li>
          </ul>
        </div>
      </nav>
  </header>
  <div class="signin">

  <div class="uk-container">
    <div class="login-form">

      <h1 class="uk-text-center">Login</h1>
      <hr>
      <form class="uk-form-stacked" id="signin" name="signin" method="post" action="/signin">
        <div class="uk-margin">
          <label for="email">Email Address</label>
          <input class="uk-input uk-border-rounded" name="email" type="text" />
        </div>
        <div class="uk-margin">
          <label for="password">Password</label>
          <input class="uk-input uk-border-rounded" name="password" type="password" />
        </div>
        <div class="uk-margin">
          <input class="uk-button uk-button-primary uk-width-1-1" type="submit" value="Sign In" />
        </div>
      </form>
    </div>
  </div>

</div>
  <footer>
    <div class="uk-container">
      <div class="copyright">
        <div class="uk-child-width-1-3@m" uk-grid>
          <div>
            <p>
              <strong>Copyright &copy; 2018-2019</strong><br /> Neighborbrew | All Rights Reserved.
            </p>
          </div>
          <div>
            <ul>
              <li><a class="grey-text text-lighten-3" href="#contact-modal" uk-toggle>Contact</a></li>
              <li><a class="grey-text text-lighten-3" href="#content-modal" uk-toggle>Content Policy</a></li>
              <li><a class="grey-text text-lighten-3" href="#legal-modal" uk-toggle>Legal</a></li>
              <li><a class="grey-text text-lighten-3" href="#disclaimer-modal" uk-toggle>Disclaimer</a></li>
            </ul>
          </div>
          <div>
            <span class="social"><a href="#"><i class="fab fa-facebook-square"></i></a></span>
            <span class="social"><a href="#"><i class="fab fa-twitter-square"></i></a></span>
            <span class="social"><a href="#"><i class="fab fa-instagram"></i></a></span>
          </div>
        </div>
      </div>
    </div>
</footer>


    <!-- UIkit JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-beta.41/js/uikit.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.0.0-beta.41/js/uikit-icons.min.js"></script>

    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.3.1.js" integrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60=" crossorigin="anonymous"></script>

    <!-- Custom JS -->
    <script src="assets/js/app.js"></script>
</body>

</html>
